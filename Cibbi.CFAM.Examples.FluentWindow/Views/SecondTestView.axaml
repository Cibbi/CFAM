<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:Cibbi.CFAM.Examples.FluentWindow.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Cibbi.CFAM.Examples.FluentWindow.Views.SecondTestView">
  <Design.DataContext>
    <viewModels:SecondTestViewModel/>
  </Design.DataContext>
  <StackPanel>
    <NumericUpDown Value="{Binding OpenPaneWidth }"></NumericUpDown>
    <NumericUpDown Value="{Binding ClosedPaneWidth }"></NumericUpDown>
  </StackPanel>
</UserControl>
