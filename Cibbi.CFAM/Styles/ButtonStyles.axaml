<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ui="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia">
    <Design.PreviewWith>
        <Border Padding="20">
            <StackPanel Spacing="5">
                <Button Content="Button" />
                <Button Content="Button" Classes="accent" />
                <Button Content="Button" Classes="accent2" />
                <Button Content="Button" Classes="cancel" />
                <Button Content="Button" Classes="cancel2" />
                <Button Content="Button" Classes="cancel" IsEnabled="False" />
                <Button Content="Button" IsEnabled="False" />
                <Border Background="Gray" Height="1" />
                <Button Content="AppBarButtonStyle" Classes="AppBarButton" />
            </StackPanel>
        </Border>
    </Design.PreviewWith>
    
    <Style Selector="Button.accent2 /template/ Border#BorderElement,
		          ui|Button.accent2 /template/ Border#BorderElement">
        <Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrush}" />
    </Style>
    <Style Selector="Button.accent2 /template/ ContentPresenter#PART_ContentPresenter,
		          ui|Button.accent2 /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource AccentButtonBackground}" />
        <!--<Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrush}" />-->
        <Setter Property="TextBlock.Foreground" Value="{DynamicResource ButtonForeground}" />
    </Style>

    <Style Selector="Button.accent2:pointerover /template/ Border#BorderElement,
		          ui|Button.accent2:pointerover /template/ Border#BorderElement">
        <Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrushPointerOver}" />
    </Style>
    <Style Selector="Button.accent2:pointerover /template/ ContentPresenter#PART_ContentPresenter,
		          ui|Button.accent2:pointerover /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource AccentButtonBackgroundPointerOver}" />
        <!--<Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrushPointerOver}" />-->
        <Setter Property="TextBlock.Foreground" Value="{DynamicResource ButtonForegroundPointerOver}" />
    </Style>

    <Style Selector="Button.accent2:pressed /template/ Border#BorderElement,
		          ui|Button.accent2:pressed /template/ Border#BorderElement">
        <Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrushPressed}" />
    </Style>
    <Style Selector="Button.accent2:pressed /template/ ContentPresenter#PART_ContentPresenter,
		          ui|Button.accent2:pressed /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource AccentButtonBackgroundPressed}" />
        <!--<Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrushPressed}" />-->
        <Setter Property="TextBlock.Foreground" Value="{DynamicResource ButtonForegroundPressed}" />
    </Style>

    <Style Selector="Button.accent2:disabled /template/ Border#BorderElement,
		          ui|Button.accent2:disabled /template/ Border#BorderElement">
        <Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrushDisabled}" />
    </Style>
    <Style Selector="Button.accent2:disabled /template/ ContentPresenter#PART_ContentPresenter,
		          ui|Button.accent2:disabled /template/ ContentPresenter#PART_ContentPresenter">
        <Setter Property="Background" Value="{DynamicResource AccentButtonBackgroundDisabled}" />
        <!--<Setter Property="BorderBrush" Value="{DynamicResource AccentButtonBorderBrushDisabled}" />-->
        <Setter Property="TextBlock.Foreground" Value="{DynamicResource ButtonForegroundDisabled}" />
    </Style>
    
    
    
    
