<views:CFAMUserControl x:TypeArguments="viewModels:TestContentViewModel" xmlns="https://github.com/avaloniaui"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:views="clr-namespace:Cibbi.CFAM.Views;assembly=Cibbi.CFAM"
                       xmlns:viewModels="clr-namespace:Cibbi.CFAM.Examples.SimpleWindow.ViewModels"
                       xmlns:reactiveUi="http://reactiveui.net"
                       xmlns:avalonia="clr-namespace:Cibbi.CFAM.Examples.SimpleWindow"
                       mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                       x:Class="Cibbi.CFAM.Examples.SimpleWindow.Views.TestContentView">
  <reactiveUi:ViewModelViewHost
    ViewModel="{Binding TestObject}"
    ViewLocator="{x:Static avalonia:ViewLocator.Current}">
    
  </reactiveUi:ViewModelViewHost>
</views:CFAMUserControl>
